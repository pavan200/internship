#include <stdio.h>
int main()
{
 int arr[5][2] = {{0, 2}, {0, 1}, {1, 3}, {1, 4}, {3, 4}};
 int source; 
 int dest; 
 int count; 
 int range = 5; 
 int nodes[range + 1][range - 1];
 int lens[range];

 for (int i = 0; i < range; i++)
 {
  lens[i] = 0;
 }
 for (int i = 0; i< range; i++)
 {
  source = arr[i][0];
  dest = arr[i][1];
  count = lens[source];
  lens[source] = lens[source] + 1;
  nodes[source][count] = dest;

  count = lens[dest];
  lens[dest] = lens[dest] + 1;
  nodes[dest][count] = source;
 }


 for(int i=0;i<range;i++){
  for (int j = 0; j < lens[i]; j++)
  {
   printf("number is %d %d %d\n", i, j, nodes[i][j]);
  } 
 }
  for(int i=0;i<range;i++){
    if (len[i]==1){
        prinf("%d %d",i,nodes[i][0]);
        
 
 
 return 0;
}
